{
  "name": "yanselmask/laravel-hooks",
  "description": "The WordPress filter, action system in Laravel",
  "keywords": [
        "laravel",
        "yanselmask",
        "wordpress",
        "action",
        "filter",
        "action",
        "filters",
        "actions",
        "hook",
        "hooks"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yanselmask",
            "email": "info@yanselmask.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
    "orchestra/testbench": "^9.2",
    "phpunit/phpunit": "^11.3"
  },
    "autoload": {
        "psr-4": {
            "Yanselmask\\LaravelHooks\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Yanselmask\\LaravelHooks\\Test\\": "tests",
            "Yanselmask\\LaravelHooks\\Test\\App\\": "tests/laravel/app"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yanselmask\\LaravelHooks\\LaravelHooksServiceProvider"
            ]
        }
    }
}
